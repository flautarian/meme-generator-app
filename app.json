{
  "expo": {
    "name": "meme-generator-app",
    "slug": "meme-generator-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.giacconidev.memegeneratorapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "sourceDir": "node_modules/react-native-sqlite-storage/platforms/android-native",
      "packageImportPath": "import io.liteglue.SQLitePluginPackage;",
      "packageInstance": "new SQLitePluginPackage()",
      "package": "com.giacconidev.memegeneratorapp"
    },
    "web": {
      "bundler": "metro",
      "favicon": "src/assets/favicon.png"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a7bc06c7-ead9-4019-9950-5cd34d4fc748"
      }
    }
  }
}
